{
  "name": "generator-alchmy-cli",
  "version": "0.0.01",
  "description": "a generator for mixing web components and constructing distributed apps. Utilizes Polymer IPFS Node Express.",
  "homepage": "https://alchmy.github.io/alchmy-cli",
  "author": "Raymond Smith",
  "keywords": [
    "alchmy",
    "distributed",
    "decentralized",
    "express",
    "IPFS",
    "node",
    "polymer",
    "web components",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@babel/core": "*7.0.0-beta.40",
    "@babel/preset-env": "*7.0.0-beta.40",
    "@webcomponents/webcomponentsjs": "^1.1.0",
    "babel-loader": "*7.1.4",
    "html-webpack-plugin": "^3.0.6",
    "nsp": "^2.6.3",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0",
    "yeoman-generator": "^2.0.1",
    "yo": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "scripts": {
    "alchmy": "node ./lib/alchmy.js",
